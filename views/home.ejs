<%- include('partials/header') %> 
<h1>Home</h1>
<p><%= homeContent %> </p>
<% postsArray.forEach( element => { %>
    <h1><%= element.postTitle %></h1>
    <% if (element.postContent.length > 100) { %>
        <p class="truncate"><%= element.postContent %> </p> <a href="/posts/:<%= element.postTitle %> ">Read More...</a>   
    <% } else{%>
        <p><%= element.postContent %></p> <a href="/posts/:<%= element.postTitle %>" >Read More...</a>
<% }}); %>
<%- include('partials/footer') %> 